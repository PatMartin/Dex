{
  "dateFormats" : [
    {
      "name"   : "MM/DD/yyyy",
      "pattern": "^((0[1-9])|(1[0-2]))/((0[1-9])|([12][0-9])|(3[01]))/\\d{4}$",
      "format" : "MM/DD/yyyy"
    },
    {
      "name"   : "MM-DD-yyyy",
      "pattern": "^((0[1-9])|(1[0-2]))-((0[1-9])|([12][0-9])|(3[01]))-\\d{4}$",
      "format" : "MM-DD-yyyy"
    }
  ]
}